{
  "compilerOptions": {
    "baseUrl": "./src",
    "plugins": [
      {
        "name": "typescript-snapshots-plugin",
        "snapshotDir": "./",
        "snapshotCallIdentifiers": [
          "toMatchSnapshot",
          "toThrowErrorMatchingSnapshot",
          "matchSnapshot",
        ]
      }
    ],
  }
}
